print(potega)

tekst = "CDV"
print (tekst * 2)

#pobieranie
imie=input()
print("Twoje imie to: " + imie)

#konkatenacja
nazwisko= "Nowak"
print("Twoje imie i nazwisko to: " + imie + " " + nazwisko)

dlugosc=len(imie)
print(dlugosc)
print(type(dlugosc))
dlugosc = str(dlugosc)
print(type(dlugosc))
print("Dlugosc imienia: " + dlugosc)

wiek = "20"
print("Wiek to: " + wiek)

wiekJulia = 20
print(type(wiekJulia))
wiekJulia = str(wiekJulia)

nazwisko = "Wysocki"
pierwszyZnak = nazwisko[0]
print(pierwszyZnak)

ostatniZnak = nazwisko[len(nazwisko)-1]
print(ostatniZnak)
ostatniZnak = nazwisko[-1]
print(ostatniZnak)

#konwersja
x = "5"
print(type(x))
print(x)
x = int(x)
print(type(x))
print(x)

x=float(x)
print(type(x))
print(x)

y=5
print(type(y))
y = y/2
print(type(y))
print(y)

nazwisko = "Kowalski"
print(nazwisko[0])
print(nazwisko[0:3])
print(nazwisko[-2])
print(nazwisko[-2:])
print(nazwisko[:-2])
print(nazwisko[:-2:2])